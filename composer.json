{
    "name": "joedixon/laravel-translation",
    "description": "A tool for managing all of your Laravel translations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Dixon",
            "email": "hello@joedixon.co.uk"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0||^9.0||^10.0||^11.0",
        "laravel/legacy-factories": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.0.0|^1.5.1|^1.6"
    },
    "autoload": {
        "psr-4": {
            "JoeDixon\\Translation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoeDixon\\Translation\\TranslationServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/joedixon/laravel-translation-core"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
